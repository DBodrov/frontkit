{
    "name": "@a3/uikit",
    "version": "0.5.8",
    "description": "A-3 React UI Kit",
    "main": "./dist/a3-uikit.esm.js",
    "module": "./dist/a3-uikit.esm.js",
    "dependencies": {
        "classnames": "^2.2.6",
        "element-closest": "^3.0.2",
        "react-custom-scrollbars": "^4.2.1",
        "react-input-mask": "^2.0.4",
        "react-string-clamp": "^0.3.1"
    },
    "peerDependencies": {
        "react": "^16.11.0",
        "react-dom": "^16.11.0"
    },
    "scripts": {
        "start": "start-storybook -p 9009 -s public",
        "build": "yarn build:lib",
        "build:lib": "yarn build:rollup && yarn build:dts",
        "build:rollup": "yarn rollup -c",
        "build:dts": "yarn tsc -p tsconfig.dts.json",
        "build:storybook": "build-storybook -s public",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "clean": "rimraf dist",
        "lint:eslint": "eslint --fix",
        "lint:prettier": "prettier --write"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@babel/plugin-proposal-class-properties": "^7.7.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/preset-typescript": "^7.7.2",
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@storybook/addon-actions": "^5.1.11",
        "@storybook/addon-info": "^5.1.11",
        "@storybook/addon-links": "^5.1.11",
        "@storybook/addons": "^5.1.11",
        "@storybook/react": "^5.1.11",
        "@testing-library/jest-dom": "^4.2.3",
        "@testing-library/react": "^9.3.2",
        "@types/classnames": "^2.2.9",
        "@types/jest": "^24.0.21",
        "@types/node": "12.11.1",
        "@types/react": "16.9.9",
        "@types/react-custom-scrollbars": "^4.0.6",
        "@types/react-dom": "16.9.2",
        "@types/react-input-mask": "^2.0.5",
        "@types/storybook__addon-info": "^4.1.2",
        "@types/storybook__react": "^4.0.2",
        "@typescript-eslint/eslint-plugin": "^2.6.1",
        "@typescript-eslint/parser": "^2.6.1",
        "babel-loader": "^8.0.6",
        "cz-customizable": "^6.2.0",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.5.0",
        "eslint-plugin-jest": "^23.0.2",
        "eslint-plugin-react": "^7.17.0",
        "husky": "^3.0.9",
        "jest": "^24.9.0",
        "jest-css-modules-transform": "^3.1.0",
        "lint-staged": "^9.4.2",
        "postcss-modules": "^1.4.1",
        "prettier": "^1.18.2",
        "react-docgen-typescript-loader": "^3.0.1",
        "react-scripts": "3.2.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.26.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript": "^1.0.1",
        "ts-jest": "^24.1.0",
        "tslib": "^1.10.0",
        "typescript": "^3.7.2"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": "commitizen.js"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "yarn commitlint -E HUSKY_GIT_PARAMS -g commitlint.js",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "!(*test).{ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "files": [
        "dist"
    ],
    "types": "./dist/types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://git.a-3.ru/frontend/a3-uikit.git"
    },
    "keywords": [
        "uikit",
        "react",
        "components"
    ],
    "maintainers": [
        {
            "name": "Andrey Vassilyev",
            "email": "vasiliev@a-3.ru"
        },
        {
            "name": "Ilya Sharonov",
            "email": "sharonov@a-3.ru"
        },
        {
            "name": "Sergey Puhalenko",
            "email": "puhalenko@a-3.ru"
        },
        {
            "name": "Timofey Polonsky",
            "email": "polonsky@a-3.ru"
        }
    ],
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://git.a-3.ru/frontend/a3-uikit/issues"
    },
    "homepage": "https://git.a-3.ru/frontend/a3-uikit.git"
}
